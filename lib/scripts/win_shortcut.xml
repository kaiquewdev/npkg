<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="$APP_NAME" location="startMenu"/>
  <shortcut
    name="$APP_NAME"
    target="$INSTALL_PATH\run.bat"
    commandLine=""
    workingDirectory="$INSTALL_PATH"
    description="$DESCRIPTION"
    iconFile="$INSTALL_PATH\app.ico"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="yes"
    startup="no">
  </shortcut>
</shortcuts>

